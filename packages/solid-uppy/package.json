{
  "description": "SolidJS bindings for Uppy",
  "repository": {
    "url": "https://github.com/lxsmnsyc/solid-uppy.git",
    "type": "git"
  },
  "homepage": "https://github.com/lxsmnsyc/solid-uppy/tree/main/packages/solid-uppy",
  "bugs": {
    "url": "https://github.com/lxsmnsyc/solid-uppy/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Alexis Munsayac",
  "private": false,
  "version": "0.2.1",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/cjs/production/index.js",
  "module": "./dist/esm/production/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "development": {
        "require": "./dist/cjs/development/index.js",
        "import": "./dist/esm/development/index.js"
      },
      "require": "./dist/cjs/production/index.js",
      "import": "./dist/esm/production/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "license": "MIT",
  "keywords": [
    "pridepack"
  ],
  "name": "solid-uppy",
  "devDependencies": {
    "@types/node": "^22.13.1",
    "@uppy/core": "^4.4.2",
    "pridepack": "^2.6.4",
    "solid-js": "^1.9.4",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@uppy/core": "^4",
    "solid-js": "^1.9"
  },
  "scripts": {
    "prepublish": "pridepack clean && pridepack build",
    "build": "pridepack build",
    "type-check": "pridepack check",
    "lint": "pridepack lint",
    "test": "pridepack test --passWithNoTests",
    "clean": "pridepack clean",
    "watch": "pridepack watch"
  },
  "typesVersions": {
    "*": {}
  }
}
